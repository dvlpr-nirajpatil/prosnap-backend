          "name": "sign-in",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "// Parse response JSON",
                  "const responseJson = pm.response.json();",
                  "",
                  "// Check status success",
                  "if (responseJson.status === 200) {",
                  "    ",
                  "    const accessToken = responseJson.data.accessToken;",
                  "    const refreshToken = responseJson.data.refreshToken;",
                  "",
                  "    // Set collection variables",
                  "    pm.collectionVariables.set(\"accessToken\", accessToken);",
                  "    pm.collectionVariables.set(\"refreshToken\", refreshToken);",
                  "",
                  "    console.log(\"‚úÖ Tokens saved to collection variables\");",
                  "} else {",
                  "    console.log(\"‚ùå Login failed, tokens not saved\");",
                  "}"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            }
          ],
